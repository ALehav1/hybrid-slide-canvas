# Environment Variables Template
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Application Environment
VITE_APP_ENVIRONMENT=development

# Optional: Application Version (auto-populated from package.json)
VITE_APP_VERSION=0.0.0

# Optional: Debug Configuration
VITE_DEBUG_MODE=false

# Deployment Configuration (for CI/CD)
# These are typically set in your deployment platform's environment settings
# VERCEL_TOKEN=your_vercel_token
# VERCEL_ORG_ID=your_vercel_org_id  
# VERCEL_PROJECT_ID=your_vercel_project_id
# SNYK_TOKEN=your_snyk_security_token
